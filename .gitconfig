[user]
    email = jdunk@users.noreply.github.com
    name = jdunk
[alias]
    abandon = reset --hard
    br = branch
    ci = commit
    co = checkout
    d = diff
    dc = diff --cached
    last = log -1 HEAD
    lol = log --oneline --graph --decorate --all
    pr = pull --rebase
    s = status
    st = status
    stash-unapply = !git stash show -p | git apply -R
    undo-commit = reset --soft HEAD^
    unstage = reset HEAD --
[branch]
    autosetuprebase = always
[color]
    diff = true
    ui = auto
[push]
    default = matching
[filter "media"]
    clean = git-media-clean %f
    smudge = git-media-smudge %f
[credential]
    helper = cache --timeout=900
[core]
    excludesfile = ~/.gitignore_global
