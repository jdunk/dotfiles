[user]
	email = jdunk@users.noreply.github.com
	name = jdunk
[alias]
	co = checkout
	br = branch
	s = status
	st = status
	ci = commit
	unstage = reset HEAD --
	undo-commit = reset --soft HEAD^
    last = log -1 HEAD
	stash-unapply = !git stash show -p | git apply -R
	d = diff
	dc = diff --cached
	abandon = reset --hard
	pr = pull --rebase
[color]
	diff = true
[push]
	default = matching
